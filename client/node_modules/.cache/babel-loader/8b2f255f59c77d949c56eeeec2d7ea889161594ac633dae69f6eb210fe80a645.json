ck=\"exit()\">Exit</button>\r\n    <h2>Home</h2>\r\n    <div style=\"display: flex;\">\r\n        <p style=\"margin: 0 0 15px 0;\">One mes</p>\r\n        <input style=\"margin: 1px 0 0 5px;\" class=\"UIKV-switch\" type=\"checkbox\" :checked=\"oneMes\" @change=\"(e)=>{updateOneMes(e.target.checked)}\">\r\n    </div>\r\n    <div class=\"item-container\" v-for=\"el in items\" :key=\"el\">\r\n        <div style=\"width: 300px;\">\r\n            {{ el.name }} {{ el.quality?'('+el.quality+')':'' }}\r\n        </div>\r\n            <div>\r\n            üîî <input class=\"UIKV-switch\" @change=\"(e)=>{updateSettings(e.target.id, e.target.checked, e.target.getAttribute('name'), e.target.getAttribute('quality'))}\" id=\"notifications\" :name=\"el.name\" :quality=\"el.quality\" :checked=\"el.notifications\" type=\"checkbox\">\r\n            ‚öñÔ∏è <input class=\"UIKV-switch\" @change=\"(e)=>{updateSettings(e.target.id, e.target.checked, e.target.getAttribute('name'), e.target.getAttribute('quality'))}\" id=\"sendEqual\" :name=\"el.name\" :quality=\"el.quality\" :checked=\"el.sendEqual\" type=\"checkbox\">\r\n            üí∏ <input class=\"UIKV-switch\" @change=\"(e)=>{updateSettings(e.target.id, e.target.checked, e.target.getAttribute('name'), e.target.getAttribute('quality'))}\" id=\"sendDifInCur\" :name=\"el.name\" :quality=\"el.quality\" :checked=\"el.sendDifInCur\" type=\"checkbox\">\r\n            üìä <input class=\"UIKV-switch\" @change=\"(e)=>{updateSettings(e.target.id, e.target.checked, e.target.getAttribute('name'), e.target.getAttribute('quality'))}\" id=\"sendDifInPer\" :name=\"el.name\" :quality=\"el.quality\" :checked=\"el.sendDifInPer\" type=\"checkbox\">\r\n        </div>\r\n        <img style=\"width: 150px;\" :src=\"imgs[el.name][el.quality]\">\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport services from '@/services/services'\r\nexport default {\r\n    data() {\r\n        return {\r\n            user: '',\r\n            items: [],\r\n            oneMes: true,\r\n            imgs: {}\r\n        }\r\n    },\r\n    methods: {\r\n        updateSettings(targetId, checked, name, quality) {\r\n            services.cahngeSettings(this.user, targetId, checked, name, quality)\r\n        },\r\n        updateOneMes(checked) {\r\n            services.cahngeOneMes(this.user, 'oneMes', checked)\r\n        },\r\n        async getImg(name, quality) {\r\n            let response = await services.getImg(name, quality)\r\n            let img = response.data\r\n            return img\r\n        },\r\n        test() {\r\n            services.sendTest()\r\n        },\r\n        exit() {\r\n            this.$router.push('/')\r\n        }\r\n    },\r\n    async mounted(){\r\n        this.user = {\r\n            code: this.$route.query.user,\r\n            tgToken: this.$route.query.tgToken\r\n        }\r\n        let resValidation = await services.tgValidate(this.user.code, this.user.tgToken)\r\n        if (!resValidation.data) {\r\n            this.$router.push('/')\r\n        }\r\n        let items = await services.getItems(this.user.code, this.user.tgToken)\r\n        this.items = items.data[0]\r\n        this.oneMes = items.data[1]\r\n\r\n        this.items.forEach(async el=>{\r\n            this.imgs[el.name] = {}\r\n            this.imgs[el.name][el.quality] = await this.getImg(el.name, el.quality)\r\n        })\r\n    }\r\n}\r\n</script>\r\n\r\n<style>\r\n:root {\r\n    --light-color: #D3E4F2;\r\n    --dark-color: #6B98BD;\r\n}\r\n.UIKV-switch {\r\n    -webkit-appearance: none;\r\n    appearance: none;\r\n    width: 35px;\r\n    height: 18px;\r\n    background-color: var(--light-color);\r\n    border-radius: 25px;\r\n    transition: all 0.3s;\r\n    cursor: pointer;\r\n    position: relative;\r\n}\r\n.UIKV-switch::after{\r\n    content: '';\r\n    width: 14px;\r\n    height: 14px;\r\n    border-radius: 50%;\r\n    background-color: #fff;\r\n    position: absolute;\r\n    top: 50%;\r\n    left: 30%;\r\n    transform: translate(-50%, -50%);\r\n    transition: all 0.3s;\r\n\r\n}\r\n.UIKV-switch:checked{\r\n    background: var(--dark-color);\r\n}\r\n.UIKV-switch:checked::after{\r\n    left: 70%;\r\n}\r\n.item-container {\r\n    display: flex;\r\n    align-items: center;\r\n    justify-content: space-evenly;\r\n}\r\n</style>"],"mappings":";AAyBA,OAAOA,QAAO,MAAO,qBAAoB;AACzC,eAAe;EACXC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,IAAI,EAAE,EAAE;MACRC,KAAK,EAAE,EAAE;MACTC,MAAM,EAAE,IAAI;MACZC,IAAI,EAAE,CAAC;IACX;EACJ,CAAC;EACDC,OAAO,EAAE;IACLC,cAAcA,CAACC,QAAQ,EAAEC,OAAO,EAAEC,IAAI,EAAEC,OAAO,EAAE;MAC7CX,QAAQ,CAACY,cAAc,CAAC,IAAI,CAACV,IAAI,EAAEM,QAAQ,EAAEC,OAAO,EAAEC,IAAI,EAAEC,OAAO;IACvE,CAAC;IACDE,YAAYA,CAACJ,OAAO,EAAE;MAClBT,QAAQ,CAACc,YAAY,CAAC,IAAI,CAACZ,IAAI,EAAE,QAAQ,EAAEO,OAAO;IACtD,CAAC;IACD,MAAMM,MAAMA,CAACL,IAAI,EAAEC,OAAO,EAAE;MACxB,IAAIK,QAAO,GAAI,MAAMhB,QAAQ,CAACe,MAAM,CAACL,IAAI,EAAEC,OAAO;MAClD,IAAIM,GAAE,GAAID,QAAQ,CAACf,IAAG;MACtB,OAAOgB,GAAE;IACb,CAAC;IACDC,IAAIA,CAAA,EAAG;MACHlB,QAAQ,CAACmB,QAAQ,CAAC;IACtB,CAAC;IACDC,IAAIA,CAAA,EAAG;MACH,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,GAAG;IACzB;EACJ,CAAC;EACD,MAAMC,OAAOA,CAAA,EAAE;IACX,IAAI,CAACrB,IAAG,GAAI;MACRsB,IAAI,EAAE,IAAI,CAACC,MAAM,CAACC,KAAK,CAACxB,IAAI;MAC5ByB,OAAO,EAAE,IAAI,CAACF,MAAM,CAACC,KAAK,CAACC;IAC/B;IACA,IAAIC,aAAY,GAAI,MAAM5B,QAAQ,CAAC6B,UAAU,CAAC,IAAI,CAAC3B,IAAI,CAACsB,IAAI,EAAE,IAAI,CAACtB,IAAI,CAACyB,OAAO;IAC/E,IAAI,CAACC,aAAa,CAAC3B,IAAI,EAAE;MACrB,IAAI,CAACoB,OAAO,CAACC,IAAI,CAAC,GAAG;IACzB;IACA,IAAInB,KAAI,GAAI,MAAMH,QAAQ,CAAC8B,QAAQ,CAAC,IAAI,CAAC5B,IAAI,CAACsB,IAAI,EAAE,IAAI,CAACtB,IAAI,CAACyB,OAAO;IACrE,IAAI,CAACxB,KAAI,GAAIA,KAAK,CAACF,IAAI,CAAC,CAAC;IACzB,IAAI,CAACG,MAAK,GAAID,KAAK,CAACF,IAAI,CAAC,CAAC;IAE1B,IAAI,CAACE,KAAK,CAAC4B,OAAO,CAAC,MAAMC,EAAE,IAAE;MACzB,IAAI,CAAC3B,IAAI,CAAC2B,EAAE,CAACtB,IAAI,IAAI,CAAC;MACtB,IAAI,CAACL,IAAI,CAAC2B,EAAE,CAACtB,IAAI,CAAC,CAACsB,EAAE,CAACrB,OAAO,IAAI,MAAM,IAAI,CAACI,MAAM,CAACiB,EAAE,CAACtB,IAAI,EAAEsB,EAAE,CAACrB,OAAO;IAC1E,CAAC;EACL;AACJ"},"metadata":{},"sourceType":